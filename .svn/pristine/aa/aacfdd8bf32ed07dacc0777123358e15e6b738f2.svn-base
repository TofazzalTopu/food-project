<%@ page import="com.docu.commons.MaritalStatus" %>
<meta name="layout" content="docuThemeRollerLayout"/>
<title><g:message code="maritalStatus.create.label" default="Create Marital Status"/></title>

<div class="main_container">
    <div class="content_container">
        <h1><g:message code="maritalStatus.create.label" default="Create Marital Status"/></h1>

        <h3><g:message code="maritalStatus.domainInfo.label" default="Marital Status Information"/></h3>
        <g:render plugin="sbicloud-commons" template='create'/>
        <div class="clear" style="height:2px; overflow:hidden; font-size:0pt;"></div>
        <g:render plugin="sbicloud-commons" template="script"/>

        <div class="jqgrid-container blue_grid">
            <table id="jqgrid-grid-maritalStatus"></table>

            <div id="jqgrid-pager-maritalStatus"></div>
        </div>

        <div id="dialog-confirm-maritalStatus" title="Delete alert" style="display: none">
            <p><span class="ui-icon ui-icon-alert"
                     style="float:left; margin:0 7px 20px 0;"></span>These item(s) will be permanently deleted and cannot be recovered. Are you sure?
            </p>
