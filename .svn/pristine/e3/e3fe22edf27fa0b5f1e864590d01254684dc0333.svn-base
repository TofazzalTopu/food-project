.swMain {
  position:relative;
  display:block;
  margin:0;
  padding:0;
  border: 0px solid #CCC;
  overflow:visible;
  float:left;
  width:812px;
}
.swMain .stepContainer {
  display:block;
  position: relative;
  margin: 0;
  padding:0;    
  border: 0px solid #CCC;    
  overflow:hidden;
  clear:both;

  color: #000000;
}

.swMain .stepContainer div.content {
  display:block;
  position: absolute;  
  float:left;
  margin: 0;
  padding:5px;    
  border: 1px solid #CCC; 
  font: normal 12px Verdana, Arial, Helvetica, sans-serif; 
  color:#000000 !important;
  background-color:#F8F8F8;  
  text-align:left;
  overflow:visible;    
  z-index:88; 
  -webkit-border-radius: 5px;
  -moz-border-radius  : 5px;
  width:800px;
  clear:both;
}

.swMain div.actionBar {
  display:block;
  position: relative; 
  clear:both;
  margin:             3px 0 0 0;   
  border:             1px solid #CCC;
  padding:            0;    
  color:              #000000;
  background-color:   #F8F8F8;
  height:40px;
  text-align:left;
  overflow:auto;    
  z-index:88; 

  -webkit-border-radius: 5px;
  -moz-border-radius  : 5px;
  left:0;
}

.swMain .stepContainer .StepTitle {
  display:block;
  position: relative;
  margin:0;   
  border:1px solid #E0E0E0;
  padding:5px;   
  font: bold 16px Verdana, Arial, Helvetica, sans-serif; 
  color:#000000;
  background-color:#E0E0E0;
  clear:both;
  text-align:left; 
  z-index:88;
  -webkit-border-radius: 5px;
  -moz-border-radius  : 5px;    
}
.swMain ul.anchor {
  position: relative;
  display:block;
  float:left;
  list-style: none;
  padding: 0px;  
  margin: 10px 0;      
  clear: both;
  border: 0px solid #CCCCCC;
  background: transparent; /*#EEEEEE */
}
.swMain ul.anchor li{ 
  position: relative; 
  display:block;
  margin: 0;
  padding: 0; 
  padding-left:3px;
  padding-right: 3px;
  border: 0px solid #E0E0E0;      
  float: left;
}
/* Anchor Element Style */
.swMain ul.anchor li a {
  display:block;
  position:relative;
  float:left;
  margin:0;
  padding:2px;
  height:20px;
  width:148px;
  text-decoration: none;
  outline-style:none;
  -moz-border-radius  : 5px;
  -webkit-border-radius: 5px;
  z-index:99;
}
.swMain ul.anchor li a .stepNumber{
  position:relative;
  float:left;
  width:auto;
  text-align: center;
  padding:5px;
  padding-top:0;
  font: bold 1em Verdana, Arial, Helvetica, sans-serif;
}
.swMain ul.anchor li a .stepDesc{
  position:relative;
  display:block;
  float:left;
  text-align: left;
  padding:5px;

  font: bold 20px Verdana, Arial, Helvetica, sans-serif;
}
.swMain ul.anchor li a .stepDesc small{
  font: normal 12px Verdana, Arial, Helvetica, sans-serif;
}
.swMain ul.anchor li a.selected{
  color:#2E2E2E;
  background: #ABD0EA;  /* EA8511 */
  border: 1px solid #3C92C8;
  cursor:text;
  /*-moz-box-shadow: 5px 5px 8px #888;*/
  /*-webkit-box-shadow: 5px 5px 8px #888;*/

}
.swMain ul.anchor li a.selected:hover {
  color:#F8F8F8;  
  background: #3C92C8;
  text-decoration: underline;
}

.swMain ul.anchor li a.done { 
  position:relative;
  color:#FFF;  
  background: #3674A0;
  border: 2px solid #3C92C8;
  z-index:99;
}
.swMain ul.anchor li a.done:hover {
  color:#F8F8F8;
  background: #3C92C8;
  border: 1px solid #000000;
  text-decoration: underline;
}
.swMain ul.anchor li a.disabled {
  color:#CCCCCC;  
  background: #F8F8F8;
  border: 1px solid #CCC;  
  cursor:text;   
}
.swMain ul.anchor li a.disabled:hover {
  color:#CCCCCC;  
  background: #F8F8F8;     
}

.swMain ul.anchor li a.error {
  color:#2E2E2E !important;
  background: #ABD0EA !important;
  border: 1px solid #3C92C8 !important;
}
.swMain ul.anchor li a.error:hover {
  color:#000 !important;
  text-decoration: underline;
}

.swMain .buttonNext {
  background-color:#88BAD7;
  display:block;
  float:right;
  margin:5px 3px 0 3px;
  padding:3px;
  text-decoration: none;
  text-align: center;
  font: bold 1em Verdana, Arial, Helvetica, sans-serif;
  width:60px;
  color:#2E2E2E;
  outline-style:none;
  border: 1px solid #68A6CF;
  -moz-border-radius  : 5px;
  -webkit-border-radius: 5px;
}
.swMain .buttonDisabled {
  color:#F8F8F8  !important;
  background-color: #D7DFE7 !important;
  border: 1px solid #CCCCCC  !important;
  cursor:text;    
}
.swMain .buttonPrevious {
  background-color:#88BAD7;
  display:block;
  float:right;
  margin:5px 3px 0 3px;
  padding:3px;
  text-decoration: none;
  text-align: center;
  font: bold 1em Verdana, Arial, Helvetica, sans-serif;
  width:60px;
  color:#2E2E2E;
  outline-style:none;

  border: 1px solid #68A6CF;
  -moz-border-radius  : 5px; 
  -webkit-border-radius: 5px;    
}
.swMain .buttonFinish {
  background-color:#88BAD7;
  display:block;
  float:right;
  margin:5px 10px 0 3px;
  padding:3px;
  text-decoration: none;
  text-align: center;
  font: bold 1em Verdana, Arial, Helvetica, sans-serif;
  width:60px;
  color:#2E2E2E;
  outline-style:none;

  border: 1px solid #68A6CF;
  -moz-border-radius  : 5px; 
  -webkit-border-radius: 5px;    
}

/* Form Styles */

.txtBox {
  border:1px solid #CCCCCC;
  color:#000000;
  font:13px Verdana,Arial,Helvetica,sans-serif;
  padding:2px;
  width:430px;
}
.txtBox:focus {
  border:1px solid #3C92C8;
}

.swMain .loader {
  position:relative;  
  display:none;
  float:left;  
  margin: 2px 0 0 2px;
  padding:8px 10px 8px 40px;
  border: 1px solid #FFD700; 
  font: bold 13px Verdana, Arial, Helvetica, sans-serif; 
  color:#000000;
  background: #FFF url(../images/loader.gif) no-repeat 5px;  
  -moz-border-radius  : 5px;
  -webkit-border-radius: 5px;
  z-index:998;
}
.swMain .msgBox {
  position:relative;  
  display:none;
  float:left;
  margin: 4px 0 0 5px;
  padding:5px;
  border: 1px solid #FFD700; 
  background-color: #FFFFDD;  
  font: normal 12px Verdana, Arial, Helvetica, sans-serif; 
  color:#000000;
  -moz-border-radius  : 5px;
  -webkit-border-radius: 5px;
  z-index:999;
  min-width:200px;  
}
.swMain .msgBox .content {
  font: normal 12px Verdana,Arial,Helvetica,sans-serif;
  padding: 0px;
  float:left;
}
.swMain .msgBox .close {
  border: 1px solid #CCC;
  border-radius: 3px;
  color: #CCC;
  display: block;
  float: right;
  margin: 0 0 0 5px;
  outline-style: none;
  padding: 0 2px 0 2px;
  position: relative;
  text-align: center;
  text-decoration: none;
}
.swMain .msgBox .close:hover{
  color: #3C92C8;
  border: 1px solid #3C92C8;
}