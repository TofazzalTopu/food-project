    border-bottom: 1px solid #DCDCDC;
}

.header_container .welcome_container ul li li.last {
    border-bottom: none;
}

.header_container .welcome_container ul li li a {
    display: block;
}

.header_container .welcome_container ul li li:hover {
    background: #E3E0E0;
}

.header_container .welcome_container ul li li a:hover {
    color: #000000;
    text-decoration: none;
}

