}

.header_container .welcome_container .text {
    float: left;
    margin: 4px 5px 0 0;
}

.header_container .welcome_container ul {
    margin: 0;
    padding: 0;
    float: left;
    width:55px;
    z-index: 10;
}

.header_container .welcome_container ul li {
    list-style: none;
    float: left;
    width: 25px;
}

.header_container .welcome_container ul li ul {
    padding: 3px;
}

.header_container .welcome_container ul li li {
    float: left;
    clear: both;
    width: 110px;
