body {
    margin: 0;
    padding: 0;
    font-family: "Verdana", "Arial", Geneva, sans-serif;
    font-size: 10px;
    color: #000000;
}

/*a, img , h1, h2, h3, h4, h5, input
{
    behavior: url(iepngfix.htc);
}*/
a, img {
    border: none;
}

a {
    text-decoration: none;
}

p, ul, li {
    margin: 0;
    padding: 0;
}

h1, h2, h3, h4 {
    margin: 0;
    padding: 0;
}

.body_container {
    width: 100%;
    position: absolute;
    left: 0;
    top: 0;
}


.header_container {
    float: left;
    clear: both;
    padding: 0;
    width: 100%; /*background:url(../images/header_bg.png) top center repeat-x;*/
    min-width: 600px;
}
.header_top_bg{
    z-index: 5;
}
.header_container .left {
    float: left;
}

.header_container h1 {
    float: left;
    padding-left: .5%;
}

.header_container h1 a {
    float: left;
    clear: both;
}

.header_container .right {
    float: right;
    width:297px;
}

.toggle_wrapper {
    position: relative;
    width:100%;
    z-index:3;
    float:left;
    clear:both;
    bottom:-5px;
    right: 8px
}

.header_container .right .acc_info_container {
    width: 290px;
    cursor: pointer;
    position:absolute;
    top: 0;
    left: 0;
    z-index:300;
}

.acc_info_container .acc_info_detail_top {
    float: left;
    clear: both;
    width: 287px;
    height: 7px;
    background: url(../images/ac_info_detail_top.png) left bottom no-repeat;
}

.acc_info_container .acc_info_detail_middle {
    float: left;
    clear: both;
    width: 290px;
    background: url(../images/ac_info_detail_middle.png) left top repeat-y;
    padding: 0px 5px 0 15px;
    display: table;
}

.acc_info_container .acc_info_detail_middle .txt_container {
    float: left;
    width: 252px;
}

.acc_info_container .acc_info_detail_middle .txt_container p {
    line-height: 20px;
    padding-top: 3px;
}

.acc_info_container .acc_info_detail_middle a {
    float: right;
    width: 15px;
    text-align: center;
}

.acc_info_container .acc_info_detail_bottom {
    float: left;
    clear: both;
    width: 287px;
    height: 7px;
    background: url(../images/ac_info_detail_bottom.png) left top no-repeat;
}

.header_container .right .search_container {
    float: left;
    clear: both;
    padding: 1px 8px 0px 0;
    width:290px;
    margin-left:-10px;

}

.header_container .right .search_container input[type="text"] {
    background: url(../images/search_input_bg.png) top left no-repeat;
    float: left;
    height: 20px;
    width: 247px;
    border: none;
    padding: 5px 10px 0px;
    font-size: 9pt;
}

.header_container .right .search_container input[type="button"] {
    background: url(../images/btn_search_bg.png) top left no-repeat;
    float: right;
    height: 26px;
    border: none;
    width: 23px;
    text-indent: -999px;
    overflow: hidden;
    font-size: 0;
    line-height: 0;
}

.header_container .welcome_container {
    float: right;
    padding-left: 45px;
    padding-top: 3px;
    /*width:300px;*/
