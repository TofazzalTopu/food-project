    border-left: 1px solid #FC8DD3!important;
}
table.pretty tbody tr.odd td:last-child, table.pretty tbody tr.even td:last-child{
    border-right: 1px solid #FC8DD3!important;
}
table.pretty tbody tr.odd td{
    background: #FFDAF1!important;
}
table.pretty tbody tr.even td{
    background: #FFCFED!important;
}
table.pretty tfoot th{
    background: #F8AFDD!important;
    border-bottom: none!important;
    border-color: #FC8DD3!important;
}
table.pretty tfoot td{
    background: #F5B9DF!important;
    border-color: #FC8DD3!important;
}
.fg-toolbar{
    background: #EB98CC!important;
    border: 1px solid #FC8DD3!important;
}
