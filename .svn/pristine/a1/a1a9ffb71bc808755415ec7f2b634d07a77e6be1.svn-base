{
    border-color: #F9DBF5!important;
}
.readonlyDiv
{
    border-bottom: 1px solid #E3C2DB!important;
    border-left: 1px solid #E3C2DB!important;
    border-right: 1px solid #E3C2DB!important;
}
body .ui-accordion .ui-accordion-content div
{
    border-bottom: 1px solid #EBD2E4!important;
}
.left-menu-section-items div a:hover
{
    color: #A03687;
}
.content_container .tab_container .tab_nav_container .tab_nav_element
{
    background: #A03687;
    border: 1px solid #E363D1;
}
.content_container .tab_container .tab_nav_container .tab_nav_element_active a
{
    color:#802C6E;
}
div a.active-link
{
    color: #E1097C !important;
}

.content_container .tab_container .tab_nav_container .tab_nav_element
{
    border: 1px solid #63AEE3;
    background:#3674A0;
}
.content_container .tab_container .tab_nav_container .tab_nav_element_active
{

}
.content_container .tab_container .tab_nav_container .tab_nav_element_active a
{
    color:#2C557F;
}

