    background-color:#95BDD6;
    border-top: 3px solid /*#95BDD6*/#D69413!important;
    border-bottom: 3px solid #D69413!important;
}
/*mb Menu end*/

/*for blue grid begin*/

.blue_grid, .blue_grid .element-title, .blue_grid select, .blue_grid option, .blue_grid input, .blue_grid h3, .blue_grid h2, .blue_grid h1, .blue_grid .element-input {
    color: #5C5C5C;
}

.blue_grid .ui-widget-content {
    border: 1px solid #d7d7d7;
}

.blue_grid .ui-widget-header {
    background: #787878;
    border: 1px solid #E0DEDC;
}

.blue_grid .ui-state-default, .blue_grid .ui-widget-content .ui-state-default, .blue_grid .ui-widget-header .ui-state-default {
    background: #B3B3B3;
    border: 1px solid #E0DEDC;
}

.blue_grid .ui-jqgrid-title {
    color: #FFFFFF;
}

.blue_grid .ui-state-default, .blue_grid .jqgrow td a, .blue_grid .ui-jqgrid tr.jqgrow td {
    color: #3C3C3C !important;
    border-right: 1px solid #E0DEDC;
}

.blue_grid tr.jqgrow {
    background: #E7E7E7;
}

.blue_grid .ui-jqgrid-btable tr.ui-priority-secondary {
    background: #F4F4F4;
    opacity: 1;
}

.blue_grid .ui-state-hover, .blue_grid .ui-widget-content .ui-state-hover, .blue_grid .ui-widget-header .ui-state-hover, .blue_grid .ui-state-focus, .blue_grid .ui-widget-content .ui-state-focus, .blue_grid .ui-widget-header .ui-state-focus {
    border: 1px solid #FC7500;
}

.blue_grid .ui-state-highlight, .blue_grid .ui-widget-content .ui-state-highlight, .blue_grid .ui-widget-header .ui-state-highlight {
    border: 1px solid #60BEEA;
}
.blue_grid fieldset {
    background: #F5F5F5;
    border: 1px solid #B3B3B3;
}

.blue_grid fieldset legend {
    background: #808080;
    color: #FFFFFF;
    border: 1px solid #D7D7D7;
}

.blue_grid fieldset .block-title {
    background: #f0f0f0;
}

.blue_grid fieldset .block-input {
    background: #ffffff;
    border-bottom: none;
}

.blue_grid .simple-table-css tr:first-child td {
    background: #808080;
    color: #FFFFFF;
}

.blue_grid .simple-table-css td, .table_container2 .simple-table-css td {
    border-top: 1px solid #D7D7D7;
    border-bottom: 1px solid #D7D7D7;
    border-left: 1px solid #D7D7D7;
    color: #5C5C5C;
}

.blue_grid .simple-table-css td a {
    font-weight: bold;
}

.blue_grid .block-title {
    background: #D5D5D5;
    color: #5C5C5C;
}

.blue_grid input[type="button"], .blue_grid .buttons input[type="button"] {
    background: #C9C8C8;
    border: 1px solid #B8B8B8;
}

/*for blue grid end*/

.sbi-layout-body-container .content_container, .sbi-layout-body-container .content_container_small, .sbi-layout-body-container .content_container_small_2
{
    border-bottom: 1px solid #FFBE85;
}
