/* for office dashboard begin */

#stylized {
    border: solid 1px #C9C9C9!important;
}
#stylized h1 {
    border-bottom: solid 1px #FFBE85!important;
}
#stylized p {
    color: #525252!important!important;
}
#stylized button {
    background: #4F4F4F!important;
}
#copyright {
    border-top: 1px solid #C9C9C9!important;
}
.footerBottom {
    border-top: 1px solid #C9C9C9!important;
}
/* for office dashboard end */

/* for combined layout begin */

.borderTop, .borderTop2 .borderTop3 .borderTop4 .borderTop5, .borderBottom, .borderBottom2, .borderBottom3, .borderBottom4, .borderBottom5, .borderLeft, .borderLeft2, .borderLeft3, .borderLeft4, .borderLeft5, .borderRight, .borderRight2, .borderRight3, .borderRight4, .borderRight5{
    border-bottom-color: #787878;
}
.bodColDark{
    border-color: #8B8B8B!important;
}

