/* mainDashboard begin */

.animation_body_container h4 {
    background: #787878;
}
.animation_body_container .odd {
    background: #F7F7F7;
}
.animation_body_container .even {
    background: #E7E7E7;
}
.animation_body_container a.details, .docu-widget-content a.details {
    color: #DE7600 !important;
}
.animation_body_container a.details:hover, .docu-widget-content a.details:hover {
    color: #6F3F09 !important;
    text-decoration: none;
}

/* mainDashboard end */

/*Dashboard portlet begin*/

.portlet .simple-table-css, .portlet .simple-table-css td {
    border-color: #C2C2C2 !important;
    color: #000000;
}

.column .simple-table-css tr.head {
    background: #B8B8B8;
}

.column .simple-table-css tr.head td {
    color: #FFFFFF !important;
}

.column .simple-table-css tr.odd {
    background: #F5F5F5;
}

.column .simple-table-css tr.even {
    background: #D5D5D5;
}

.sbi-layout-body-container .portlet-content h3 {
    background: #B8B8B8 !important;
}
.portlet-content .simple-table-css td a{
    color: #212121;
}
.portlet-content span a{
    color: #DE7600;
}
/*Dashboard portlet end*/

/* widget-content begin */

/* breadcrumb begin */

.docu-widget-breadcrumb{
    background: #EDE9E9;
}
.docu-widget-breadcrumb, .docu-widget-breadcrumb span {
    color: #DE7600;
}
.docu-widget-breadcrumb a {
    color: #DE7600;
}
.docu-widget-breadcrumb a:hover {
    color: #6F3F09;
}

/* breadcrumb end */

.docu-widget-content{
    background: #F2F2F2;
}
.docu-widget-content h4{
    color: #ffffff;
    background: #787878;
}
.docu-widget-content h4 a{
    color: #EDE9E9;
}
.docu-widget-content .odd {
    background: #F7F7F7;
}
.docu-widget-content .even {
    background: #E7E7E7;
}

/* widget-content end */


/* hr employee details begin */
.empDetail{
    border-bottom: 1px solid #ff970f;
}
.employeeDetailsContainer .icon{
    background-image: url("icon_cross.png");
    background-position: 50% 50%;
    background-repeat: no-repeat;
}
.employeeDetailsContainer .tabs > .head .normal > .left{
    background-image: url("hrEmpTabBgLeft.png");
}
.employeeDetailsContainer .tabs > .head .normal > .main, .employeeDetailsContainer .tabs > .head .normal > .xbtn{
    background-image: url("hrEmpTabBgMiddle.png");
    color: #EEEEEE;
}
.employeeDetailsContainer .tabs > .head .normal > .right{
    background-image: url("hrEmpTabBgRight.png");
}
.employeeDetailsContainer .tabs > .head .normal:hover > .left{
    background-image: url("hrEmpTabBgLeftHover.png");
}
.employeeDetailsContainer .tabs > .head .normal:hover > .main, .employeeDetailsContainer .tabs > .head .normal:hover > .xbtn{
    background-image: url("hrEmpTabBgMiddleHover.png");
    color: #FFFFFF!important;
}
.employeeDetailsContainer .tabs > .head .normal:hover > .right{
    background-image: url("hrEmpTabBgRightHover.png");
}


/* hr employee details end */
