}
legend
{
    background: #787878!important;
    border: 1px solid #DABB94!important;
}
.readonly-input, .element-input input[readonly="true"], input.readonly-input
{
    border-color: #FBDFB6!important;
}
.readonlyDiv
{
    border-bottom: 1px solid #FBDFB6!important;
    border-left: 1px solid #FBDFB6!important;
    border-right: 1px solid #FBDFB6!important;
}
body .ui-accordion .ui-accordion-content div
{
    border-bottom: 1px solid #F8E1C4!important;
}
.left-menu-section-items div a:hover
{
    color: #A35B09;
}
