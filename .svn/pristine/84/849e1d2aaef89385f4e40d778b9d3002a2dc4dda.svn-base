/* Data tables begin */
table.pretty thead th, .fg-toolbar, table.pretty tbody tr td, table.pretty tfoot th, table.pretty tfoot td{
    color: #000000!important;
}
table.pretty thead th{
    background: #D0D0D0!important;
    border-bottom: 1px solid #8B8B8B!important;
    border-left: 1px solid #8B8B8B!important;
}
table.pretty thead th:last-child{
    border-right: 1px solid #8B8B8B!important;
}
table.pretty tbody tr.odd td:first-child, table.pretty tbody tr.even td:first-child{
    border-left: 1px solid #8B8B8B!important;
}
table.pretty tbody tr.odd td:last-child, table.pretty tbody tr.even td:last-child{
    border-right: 1px solid #8B8B8B!important;
}
table.pretty tbody tr.odd td{
    background: #FBFBFB!important;
}
table.pretty tbody tr.even td{
    background: #F1F1F1!important;
}
table.pretty tfoot th{
    background: #B5B5B5!important;
    border-color: #8B8B8B!important;
}
table.pretty tfoot td{
    background: #DEDEDE!important;
    border-color: #8B8B8B!important;
}
.fg-toolbar{
    background: #B8B8B8!important;
    border: 1px solid #8B8B8B!important;
}

/* Data tables end */

