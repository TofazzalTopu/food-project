.element-content-form-elements label, .element_row_content_container label {
    background-color: #E7E7E7!important;
}
.element_row_content_container .element_content_row_container, .element-content-form-elements, .lightColorbg {
    background-color: #F7F7F7!important;
}
.big_height_lightColorbg{
    background: #F7F7F7 url("big_label_bg.png")left top repeat-y !important;
}
