/** STYLE TRANSFERRED FROM MAIN.GSP */
/* remove padding and scrolling from elements that contain an Accordion OR a content-div */
.ui-layout-center, /* has content-div */
.ui-layout-west, /* has Accordion */
.ui-layout-east, /* has content-div ... */
.ui-layout-east .ui-layout-content {
  /* content-div has Accordion */
    padding: 0;
    overflow: hidden;
}

.ui-layout-center P.ui-layout-content {
    line-height: 1.4em;
    margin: 0; /* remove top/bottom margins from <P> used as content-div */
}

h3, h4 {
  /* Headers & Footer in Center & East panes */
    font-size: 1.1em;
    background:#FFF url("../img/tab0.gif");
    border: 1px solid #BBB;
    border-width: 0 0 1px;
    padding: 9px;
    margin: 0;
}

.ui-layout-east h4 {
  /* Footer in East-pane */
    font-size: 0.9em;
    font-weight: normal;
    border-width: 1px 0 0;
}

ul.menuDiv {
	list-style: none;
	margin: 0px;
	padding: 0px;
}

li.menuDiv {
	list-style: none;
	margin: 0px;
	padding: 0px;
}

/* unvisited link */
ul.menuDiv li a:link {
	font-size: 80%;
	display: block;
	padding: 3px 0px 3px 4px;
	text-decoration: none;
	color: #666666;
	width:98%;
}

ul.menuDiv li a:visited {
	font-size: 80%;
	display: block;
	padding: 3px 0px 3px 4px;
	text-decoration: none;
	color: #666666;
	width:98%;
}
  
ul.menuDiv li a:hover {
    color: #000000;
	background-color: #eeeeff;
}

