html * {
    margin: 0;
	color:#2E6E9E;
	direction:ltr;
	font-family:"lucida grande",tahoma,verdana,arial,sans-serif;
    font-size:11px;
	text-align:left;
}

/* GENERAL */
a {
	color:#3B5998;
	cursor:pointer;
	text-decoration:none;
}

.spinner {
    padding: 5px;
    position: absolute;
    right: 0;
}

.sbi-layout-body-container{
    width: 800px;
}

/** ATHENA: FORM */
label {
	float: left;
	text-align: left;
}

form {
    clear:both;
}

form div {
    margin: .8em 0;
}

fieldset {
	-moz-border-radius:5px 5px 5px 5px;
	border:1px solid #BDC7D8;
	margin:2px 0;
	padding:15px;
	background-color:#fff;
}

legend {
	padding:3px 10px 3px 10px;
	font:1em "lucida grande",tahoma,verdana,arial,sans-serif;
	font-weight:bold;
	-moz-border-radius:5px 5px 5px 5px;
	border:1px solid #BDC7D8;
	background-color:#f0f6ff;
}

.flexigrid div.fbutton .dexcel, .flexigrid div.fbutton .process-n-download {
	background: url(../images/dexcel.png) no-repeat center left;
}

.flexigrid div.fbutton .delete {
	background: url(../images/close.png) no-repeat center left;
}

.flexigrid div.fbutton .edit {
	background: url(../images/edit.png) no-repeat center left;
}

.flexigrid div.fbutton .clear-results {
	background: url(../images/clear-results.png) no-repeat center left;
}

.spinner {
	clear:both;float:left;margin-bottom:30px;position:relative;
}

.flexigrid div.nBtn,
.flexigrid div.nBtn div {
	height:28px !important;
}

li.error {
   color:#ff0000;
   font:Arial, Helvetica, sans-serif; 
}

.error {
   color:#ff0000;
   font:Arial, Helvetica, sans-serif;
}

input[type=text].error, select.error, textarea.error{
   border-color:#f9dd34;
   background-color:#fff9d7;
   color:#000000;
}

#errorList li{
   color:#ff0000;
}

/* Larger Side Border */
.required {
   border: 1px solid #BDC7D8;
   border-left: 4px solid #BDC7D8
}

.save, .clear-form, .search {
	color:#2E6E9E;
	text-align:center;
	padding:3px 3px 3px 3px;
	font:1em "lucida grande",tahoma,verdana,arial,sans-serif;
	font-weight:bold;
	-moz-border-radius:5px 5px 5px 5px;
	border:1px solid #C5DBEC;
	background:url("images/ui-bg_glass_85_dfeffc_1x400.png") repeat-x scroll 50% 50% #DFEFFC;
}

.save:hover, .clear-form:hover, .search:hover  {
    background-color:#f1f8fd;
    outline: none;
}

.save:active, .clear-form:active, .search:active {
	padding:3px 3px 3px 3px;
	font:1em "lucida grande",tahoma,verdana,arial,sans-serif;
	font-weight:bold;
	-moz-border-radius:5px 5px 5px 5px;
	border:1px solid #2694e8;
	outline: none;
}

.buttons {
    border-bottom:1px solid #BDC7D8;
    border-top:1px solid #BDC7D8;
    clear:right;
    color:#666666;
    font:12px/1.5 Verdana;
    padding:5px 8px;
}

.loginButton {
    border-bottom:1px solid #BDC7D8;
    border-top:1px solid #BDC7D8;
    clear:right;
    color:#666666;
    font:12px/1.5 Verdana;
    padding:5px 8px;
    text-align:center
}

.deleteMsg {
    background-color:#f0e68c;
}

