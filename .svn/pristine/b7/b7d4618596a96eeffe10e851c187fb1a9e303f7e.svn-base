.docu-tree-dropdown-header{
    background: #FFF url("images/icon_dropdown.png") top right no-repeat;
    display: table;
    border-left: 1px solid #c3c3c3;
    border-right: 1px solid #c3c3c3;
    border-bottom: 1px solid #dddddd;
    border-top: 2px solid #848484;
    padding: 0 20px 0 5px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    width: 100%;
    height: 18px;
}
.docu-tree-dropdown-header:hover, .docu-hover{
    background: #FFF url("images/icon_dropdown_hov.png") top right no-repeat;
}
.docu-tree-dropdown-body{
    background: #ffffff;
    border: 1px solid;
    border-bottom-color: #000000;
    border-right-color: #000000;
    border-top-color: #CCCCCC;
    border-left-color: #CCCCCC;
    padding: 0 2px 2px;
    -webkit-border-radius-bottomleft: 5px;
    -webkit-border-radius-bottomright: 5px;
    -moz-border-radius-bottomleft: 5px;
    -moz-border-radius-bottomright: 5px;
    border-radius-bottomleft: 5px;
    border-radius-bottomright: 5px;
}
.docu-tree-dropdown-nav{
    border-top:1px solid #E8E8E8;
    padding: 3px;
    float: left;
    width: 100%;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
.docu-tree-dropdown-nav a{
    color: #000000;
    padding: 0 3px;
}
.docu-tree-dropdown-nav a:hover{
    color: #606060;
}
.docu-tree-dropdown-search-panel{
    float: right;
}
.docu-tree-dropdown-search-panel #docu-tree-dropdown-search-input{
    float: right;
    width: 70% !important;
}
.docu-tree-dropdown-search-panel span{
    background: url("images/icon-magnifier.png") center center no-repeat;
    width: 16px;
    height: 16px;
    float: right;
    margin: 3px 0 0 5px;
}
.docu-tree-dropdown-items{
    border: 1px solid #F2F2F2;
    overflow-x: auto;
    overflow-y: scroll;
}
.docu-tree-dropdown-items ul{
    padding: 10px 0;
    margin: 5px;
}
.docu-tree-dropdown-items ul li{
    list-style: none;
    background: #FBFBFB;
    border-bottom: 1px solid #DEDEDE;
    padding-top: 3px;
    padding-bottom: 3px;
}
.docu-tree-dropdown-items ul li .docu-tree-dropdown-checkbox{

}
.docu-tree-dropdown-items ul li span{
    margin-left: 5px;
}
.docu-tree-dropdown-items ul li:last-child{
    border-bottom: none;
}
.docu-tree-dropdown-items ul li ul{
    padding: 1px 20px;
    margin: 0;
    border-top: 1px solid #F1F1F1;
}
.docu-tree-dropdown-items ul li ul li{
    border-bottom: 1px solid #F1F1F1;
}

.docu-tree-dropdown-searched-node{
    background-color: #FCEFA1 !important;
}