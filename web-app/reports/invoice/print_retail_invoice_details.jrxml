	<parameter name="invoiceId" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
	finish_product.name AS `descriptionOfGoods`,
	SUM(invoice_details.`quantity`) AS `delivered_quantity`, GROUP_CONCAT(IFNULL(invoice_details.`batch_number`,'')) AS `batchNo`,
	ROUND(AVG(invoice_details.`unit_price`), 2) AS `unit_price`
FROM `invoice_details`
INNER JOIN invoice
	ON (invoice_details.`invoice_id` = invoice.id)
INNER JOIN finish_product
	ON (finish_product.id = invoice_details.finish_product_id)
WHERE invoice.id = $P{invoiceId}
GROUP BY finish_product.`id`;]]>
	</queryString>
	<field name="descriptionOfGoods" class="java.lang.String"/>
