<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="print_loading_slip_bonus" language="groovy" pageWidth="555" pageHeight="802" columnWidth="555" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="loadingSlipId" class="java.lang.Long"/>
	<queryString>
		<![CDATA[SELECT lsd.invoice_id, fp.name, qbb.batch_no, qbb.quantity
FROM loading_slip ls
INNER JOIN loading_slip_details lsd
	ON lsd.loading_slip_id = ls.id
INNER JOIN quantity_based_bonus qbb
	ON qbb.invoice_id = lsd.invoice_id
INNER JOIN finish_product fp
	ON fp.id = qbb.finish_product_id
WHERE ls.id = $P{loadingSlipId}]]>
	</queryString>
	<field name="invoice_id" class="java.lang.Long"/>
	<field name="name" class="java.lang.String"/>
	<field name="batch_no" class="java.lang.String"/>
	<field name="quantity" class="java.lang.Float"/>
